@{
    ViewData["Title"] = "Cart";
}
<div class="container-pay">
    <div class="card">
        <button class="proceed">
            <svg class="sendicon" width="24" height="24" viewBox="0 0 24 24">
                <path d="M4,11V13H16L10.5,18.5L11.92,19.92L19.84,12L11.92,4.08L10.5,5.5L16,11H4Z"></path>
            </svg>
        </button>
        <img src="data:image/png;base64," class="logo-card">
        <label>Card number:</label>
        <input id="user" type="text" class="input cardnumber" placeholder="1234 5678 9101 1121">
        <label>Name:</label>
        <input class="input name" placeholder="Name as per card" value="@(Context.Session.GetString("UserName"))">
        <label class="toleft">CCV:</label>
        <input class="input toleft ccv" placeholder="321">
    </div>
    <div class="receipt">
        <div class="col">
            <p>Cost:</p>
            <h2 class="cost">15,000 &#8377;</h2><br>
            <p>Name:</p>
            <h2 class="seller">Abhishek</h2>
        </div>
        <div class="col">
            <p>Bought Items:</p>
            <h3 class="bought-items">Eminem album</h3>
            <p class="bought-items description">Music names</p>
            <p class="bought-items price">5,000 &#8377;</p><br>
            <h3 class="bought-items">Lata mangeskar Album</h3>
            <p class="bought-items description">Music names</p>
            <p class="bought-items price">10,000 </p><br>
        </div>
    </div>
</div>